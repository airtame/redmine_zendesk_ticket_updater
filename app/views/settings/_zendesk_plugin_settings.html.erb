  <tr>
    <th>Redmine Url</th>
    <td>
      <input type="text" id="settings_redmine_url" value="<%= settings['redmine_url'] %>" name="settings[redmine_url]" />
    </td>
  </tr>
  <tr>
    <th>Redmine Custom Field ID</th>
    <td>
      <input type="text" id="settings_redmine_custom_field_id" value="<%= settings['redmine_custom_field_id'] %>" name="settings[redmine_custom_field_id]" />
    </td>
  </tr>
  <tr>
    <th>Zendesk Subdomain</th>
    <td>
